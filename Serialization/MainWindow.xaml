<Window x:Class="Serialization.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Serialization"
        mc:Ignorable="d"
        Title="" 
        FontSize="16"
        Name="TaskWindow"
        Loaded="MainWindow_OnLoaded" 
        Height="500" Width="700" 
        MinHeight="{Binding MinHeight1}" MinWidth="{Binding MinWidth1}" >

        <Frame Name="WindowFrame" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
               MinHeight="350" MinWidth="530" NavigationUIVisibility="Hidden" />
</Window>
